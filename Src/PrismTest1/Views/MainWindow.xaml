<Window x:Class="PrismTest1.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:prism="http://prismlibrary.com/"
        prism:ViewModelLocator.AutoWireViewModel="True"
        xmlns:inf="clr-namespace:PrismTest1.Infrastructure;assembly=PrismTest1.Infrastructure"
        Title="{Binding Title}" Height="350" Width="525">
    <DockPanel LastChildFill="True">
        <StackPanel Orientation="Horizontal" DockPanel.Dock="Top" prism:RegionManager.RegionName="{x:Static inf:RegionNames.ToolbarRegion}" />
        <ItemsControl DockPanel.Dock="Top" prism:RegionManager.RegionName="{x:Static inf:RegionNames.BreadcrumbRegion}"/>
        <ContentControl prism:RegionManager.RegionName="{x:Static inf:RegionNames.ContentRegion}" />
    </DockPanel>
</Window>
